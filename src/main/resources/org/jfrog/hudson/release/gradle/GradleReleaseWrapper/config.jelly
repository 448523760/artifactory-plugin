<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout"
         xmlns:t="/lib/hudson" xmlns:f="/lib/form">
    <f:entry field="tagPrefix" title="VCS tags base URL/name"
             help="/plugin/artifactory/help/release/maven/MavenReleaseWrapper/help-tagBaseUrl.html">
        <f:textbox name="tagPrefix" value="${instance.tagPrefix}"/>
    </f:entry>
    <!--<j:if test="${it.isGit()}">-->
    <f:entry field="releaseBranchPrefix" title="Release branch name prefix"
             help="/plugin/artifactory/help/release/maven/MavenReleaseWrapper/help-releaseBranchPrefix.html">
        <f:textbox name="releaseBranchPrefix" value="${instance.releaseBranchPrefix}"/>
    </f:entry>
    <!--</j:if>-->
    <f:entry title="Version property keys"
             help="/plugin/artifactory/help/release/maven/MavenReleaseWrapper/help-versionPropKeys.html">
        <f:textbox name="versionPropKeys" value="${instance.versionPropKeys}"/>
    </f:entry>
    <f:entry title="Additional property keys"
             help="/plugin/artifactory/help/release/maven/MavenReleaseWrapper/help-additionalPropKeys.html">
        <f:textbox name="additionalPropKeys" value="${instance.additionalPropKeys}"/>
    </f:entry>

    <f:entry title="Alternative Gradle tasks and options"
             help="/plugin/artifactory/help/release/gradle/GradleReleaseWrapper/help-alternativeTasks.html">
        <f:textbox name="alternativeGoals" value="${instance.alternativeGoals}"/>
    </f:entry>
</j:jelly>